generator client {
  provider = "prisma-client-js"
  engineType = "binary"
  binaryTargets = ["native", "linux-musl", "debian-openssl-1.1.x", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Customer {
  id Int @id @default(autoincrement())
  name String
  stores Store[]
}

model Store {
  id  Int @id @default(autoincrement())
  name String
  openDate DateTime
  closeDate DateTime
  markForExclude Boolean
  phone String
  streetName String
  customer Customer @relation(fields: [customerId], references: [id])
  customerId Int
  distributionCenter DistributionCenter @relation(fields: [distributionCenterId], references: [id])
  distributionCenterId Int
}

//open_date,close_date,mark_for_exclude,phone,street_name
model DistributionCenter {
  id Int @id @default(autoincrement())
  name String
  stores Store[]
}